{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
    "manifestVersion": "1.3",
    "version": "1.0.0",
    "id": "0d04d80f-1bbf-8f08-bc09-b91e2560de00",
    "packageName": "com",
    "developer": {
        "name": "<<bot name>>",
        "websiteUrl": "<<Company website Url>>",
        "privacyUrl": "<<Company privacy Url>>",
        "termsOfUseUrl": "<<Company Terms Of Use Url>>"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "<<bot name>>",
        "full": "<<bot name>>"
    },
    "description": {
        "short": "<<bot description>>",
        "full": "<<bot description>>"
    },
    "accentColor": "#FFFFFF",
    "bots": [
      {
        "botId": "<<bot id>>",
        "scopes": [
          "personal",
          "team"
        ],
        "supportsFiles": false,
        "isNotificationOnly": false
      }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "token.botframework.com",
        "*.ngrok.io",
        "*.azurewebsites.net"
    ]
}
