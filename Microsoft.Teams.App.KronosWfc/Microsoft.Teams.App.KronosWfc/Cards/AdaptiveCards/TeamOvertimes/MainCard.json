{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "Container",
      "id": "MainContainer",
      "items": [
        {
          "type": "TextBlock",
          "weight": "Bolder",
          "size": "Large",
          "text": "{txt_CardTitle}",
          "wrap": true
        },
        {
          "type": "Container",
          "spacing": "Large",
          "id": "ClosedRequestsContainer",
          "isVisible": {ShowList},
          "items": [
            {List}
          ]
        }
      ]
    },
    {
      "type": "TextBlock",
      "isVisible": {NoOverTime},
      "text": "{txt_NoOverTime}",
      "wrap": true
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0"
}